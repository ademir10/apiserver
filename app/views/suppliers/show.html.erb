<%- model_class = Supplier -%>
<div class="page-header">
  <h1>Fornecedor</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Nome:</strong></dt>
  <dd><%= @supplier.name %></dd>
  <dt><strong>CEP:</strong></dt>
  <dd><%= @supplier.cep %></dd>
  <dt><strong>Endere√ßo:</strong></dt>
  <dd><%= @supplier.address %></dd>
  <dt><strong>Bairro:</strong></dt>
  <dd><%= @supplier.neighborhood %></dd>
  <dt><strong>Cidade:</strong></dt>
  <dd><%= @supplier.city %></dd>
  <dt><strong>Estado:</strong></dt>
  <dd><%= @supplier.state %></dd>
  <dt><strong>Fone:</strong></dt>
  <dd><%= @supplier.phone %></dd>
  <dt><strong>Celular:</strong></dt>
  <dd><%= @supplier.cellphone %></dd>
  <dt><strong>CNPJ:</strong></dt>
  <dd><%= @supplier.cnpj %></dd>
  <dt><strong>Email:</strong></dt>
  <dd><%= @supplier.email %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              suppliers_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_supplier_path(@supplier), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              supplier_path(@supplier),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
